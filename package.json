{
  "name": "react-boot",
  "description": "React.js package to kickstart an app",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "immer": "^8.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/redux-mock-store": "^1.0.2",
    "connected-react-router": "^6.8.0",
    "fontsource-roboto": "^4.0.0",
    "graphql": "^15.4.0",
    "graphql.macro": "^1.4.2",
    "history": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "~4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:ci": "react-scripts test --ci --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze-json": "source-map-explorer 'build/static/js/*.js' --json",
    "_comment_predeploy": "The predeploy script will run automatically before deploy is run.",
    "predeploy": "yarn build",
    "_comment_deploy": "Publish/deploy site/build to github pages",
    "deploy": "gh-pages -d build",
    "_comment_postbuild": "react-snap",
    "_comment_serve": "Static Server: serve your static site on the port 5000",
    "serve": "serve -s build",
    "size": "yarn build && size-limit",
    "cypress:run": "cypress run --record",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:run",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@size-limit/preset-app": "^4.9.1",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/node-logger": "^6.0.27",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.27",
    "@types/jest": "^26.0.15",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.15",
    "@types/react-router": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-loader": "8.1.0",
    "cypress": "^6.3.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "react-snap": "^1.23.0",
    "react-styleguidist": "^11.1.1",
    "size-limit": "^4.9.1",
    "source-map-explorer": "^2.5.0",
    "start-server-and-test": "^1.11.7"
  },
  "_comment_proxy": "Proxying API Requests in Development",
  "_commented_proxy": "http://localhost:4000",
  "_comment_homepage": "Building for Relative Paths, correctly infer the root path to use in the generated HTML file,",
  "_commented_homepage": "https://skskumbharkar.github.io/react-boot/",
  "size-limit": [
    {
      "path": "build/static/**/*.{js,css,svg,woff,woff2,png,ttf,html}"
    }
  ]
}
